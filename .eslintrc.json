{
    "extends": [
        "sandricoprovo/typescript"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "printWidth": 80,
                "bracketSpacing": true,
                "useTabs": false,
                "semi": true,
                "endOfLine": "auto"
            }
        ],
        "react/prop-types": "off",
        "react/jsx-no-bind": "off"
    }
}